    "@typescript-eslint/parser": "^7.16.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.2.1",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
