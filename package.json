{
  "name": "@melfore/mosaic",
  "version": "0.1.45",
  "homepage": "https://github.com/melfore/mosaic#readme",
  "description": "Melfore's UI kit library based on `@material-ui`",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melfore/mosaic.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@date-io/date-fns": "^2.6.0",
    "@date-io/core": "^2.6.0",
    "date-fns": "^2.12.0",
    "material-table": "^1.57.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/prompt-cli": "^9.1.1",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.17",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.29",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "prettier": "^2.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-intl": "^4.3.1",
    "semantic-release": "^17.1.1",
    "ts-jest": "^25.2.1",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@material-ui/core": "4.9.x",
    "@material-ui/icons": "4.9.x",
    "@material-ui/lab": "4.0.0-alpha.54",
    "react": "16.13.x",
    "react-dom": "16.13.x",
    "react-intl": "4.3.x"
  },
  "scripts": {
    "build": "npm run pre-build && tsc",
    "build-storybook": "build-storybook -o docs --docs",
    "commit": "npx git-cz",
    "lintify": "eslint .",
    "pre-build": "rm -rf ./dist && cp -f ./.typescript/tsconfig.build.json ./tsconfig.json",
    "pre-test": "cp -f ./.typescript/tsconfig.test.json ./tsconfig.json",
    "prettify": "prettier --write .",
    "start": "npm run pre-test && start-storybook -p 9009",
    "test": "npm run pre-test && jest --clearCache && jest --c ./.jest/config.js --watch",
    "test-coverage": "npm run pre-test && jest --clearCache && jest --c './.jest/config.js' --coverage",
    "upload": "npm run build && npm publish --access public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
